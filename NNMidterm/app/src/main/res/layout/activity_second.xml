<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ca.on.conestogac.navjot.nnmidterm.SecondActivity">
    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <fragment class="ca.on.conestogac.navjot.nnmidterm.ImageFragment"
            android:id="@+id/imgFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:name="ca.on.conestogac.navjot.nnmidterm.ImageFragment"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            >
        </fragment>
        <fragment class="ca.on.conestogac.navjot.nnmidterm.FormFragment"
            android:id="@+id/formFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:name="ca.on.conestogac.navjot.nnmidterm.FormFragment"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/imgFragment">
        </fragment>
    </RelativeLayout>

</android.support.constraint.ConstraintLayout>
